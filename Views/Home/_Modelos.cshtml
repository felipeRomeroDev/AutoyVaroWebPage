@model AutoyVaro.com.libroazul.www.Catalogo[]

<div class="row">
    <div class="col-md-12">
        
        @foreach (var item in Model)
        {
            <a class="btn btn-default btn-outline btn-xl label-modelos" id="@("ModeloId"+item.Clave)" onclick="selectModelo(@item.Clave, '@item.Nombre')">
                @item.Nombre
            </a>
        }
    </div>
</div>

<script type="text/javascript">
    var listaModelosTemporal = @Html.Raw(Json.Encode(Model));
</script>
