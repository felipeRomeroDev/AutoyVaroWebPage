    <script type="text/javascript">

        function MostrarTerminosGlobal() {
            $.get('@Url.Action("TerminosPartial", "Home")').done(function (data) {
                $("#ModalGlobalBody").html(data);
                $("#ModalGlobal").modal("show");
            });
        }
        function MostrarAvisoGlobal() {
            $.get('@Url.Action("AvisoPartial", "Home")').done(function (data) {
                $("#ModalGlobalBody").html(data);
                $("#ModalGlobal").modal("show");
            });
        }


        function IniciarSesionPrincipal() {
            $("#ModalLoggin").modal("show");
            RegistroUser();
        }


        function SolicitudAsesoriaPrincipal() {
            $("#ModalGlobal").modal("show");
            var URL_Contacto = "@Url.Action("ContactoPartial", "Home")";
            $.get(URL_Contacto).done(function (data) {
                $("#ModalGlobal").modal("show");
                $("#ModalGlobalBody").html(data);                
            });
        }

        function LoginUser() {
            $("#tabRegistro").html("");
            $("#tabLogin").removeClass("active");
            $("#tabRegistro").removeClass("active");
            $("#tabLogin").addClass("active");
            $("#LinktabLogin").removeClass("active");
            $("#LinktabRegistro").removeClass("active");
            $("#LinktabLogin").addClass("active");

            var urlLogin ='@Url.Action("Login", "Account")';
            $.get(urlLogin).done(function (data) {
                $("#tabLogin").html(data);

            });
        }

        function RegistroUser() {
            $("#tabLogin").html("");
            $("#tabRegistro").removeClass("active");
            $("#tabLogin").removeClass("active");
            $("#tabRegistro").addClass("active");
            $("#LinktabRegistro").removeClass("active");
            $("#LinktabLogin").removeClass("active");
            $("#LinktabRegistro").addClass("active");
            var urlLogin ='@Url.Action("Register", "Account")';
            $.get(urlLogin).done(function (data) {
                $("#tabRegistro").html(data);

            });
        }

        function MostrarTerminosGlobal() {
            $.get('@Url.Action("TerminosPartial", "Home")').done(function (data) {
                $("#ModalGlobalBody").html(data);
                $("#ModalGlobal").modal("show");
            });
        }
        function MostrarAvisoGlobal() {
            $.get('@Url.Action("AvisoPartial", "Home")').done(function (data) {
                $("#ModalGlobalBody").html(data);
                $("#ModalGlobal").modal("show");
            });
        }

        $('.back-to-top-inicio').on('click', function () {
            $('html, body').animate({
                scrollTop: 0
            }, 100);
            return false;
        });




    </script>

